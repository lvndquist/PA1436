  "devDependencies": {
    "normalize.css": "^8.0.1",
    "sass": "^1.80.6",
    "stylelint": "^16.10.0",
    "stylelint-config-sass-guidelines": "^12.1.0",
    "stylelint-scss": "^6.8.1"
  }
}
