{
  "name": "themes",
  "version": "1.0.0",
  "main": "index.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "stylelint \"**/*.scss\"",
    "watch": "sass fantastic/scss/style.scss:fantastic/css/style.css fantastic/scss/style-dark.scss:fantastic/css/style-dark.css --no-source-map --watch",
    "watch-min": "sass fantastic/scss/style.scss:fantastic/css/style.min.css fantastic/scss/style-dark.scss:fantastic/css/style-dark.min.css --no-source-map --watch --style compressed",
    "style": "npm run style-light && npm run style-dark",
    "style-min": "npm run style-light-min && npm run style-dark-min",
    "style-dark": "sass fantastic/scss/style-dark.scss fantastic/css/style-dark.css --no-source-map",
    "style-dark-min": "sass fantastic/scss/style-dark.scss fantastic/css/style-dark.min.css --no-source-map --style compressed",
    "style-light": "sass fantastic/scss/style.scss fantastic/css/style.css --no-source-map",
    "style-light-min": "sass fantastic/scss/style.scss fantastic/css/style.min.css --no-source-map --style compressed"
},
